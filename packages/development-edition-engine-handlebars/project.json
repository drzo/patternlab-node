{
  "name": "development-edition-engine-handlebars",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/development-edition-engine-handlebars/src",
  "projectType": "library",
  "implicitDependencies": [
    "cli",
    "core",
    "engine-handlebars",
    "engine-mustache",
    "plugin-tab",
    "starterkit-handlebars-demo",
    "uikit-workshop"
  ],
  "targets": {
    "pl:build": {
      "executor": "nx:run-script",
      "options": {
        "script": "pl:build"
      },
      "dependsOn": [{ "target": "build", "projects": "dependencies" }]
    },
    "pl:help": {
      "executor": "nx:run-script",
      "options": {
        "script": "pl:help"
      },
      "dependsOn": [{ "target": "build", "projects": "dependencies" }]
    },
    "pl:install": {
      "executor": "nx:run-script",
      "options": {
        "script": "pl:install"
      },
      "dependsOn": [{ "target": "build", "projects": "dependencies" }]
    },
    "pl:serve": {
      "executor": "nx:run-script",
      "options": {
        "script": "pl:serve"
      },
      "dependsOn": [{ "target": "build", "projects": "dependencies" }]
    },
    "pl:starterkit": {
      "executor": "nx:run-script",
      "options": {
        "script": "pl:starterkit"
      },
      "dependsOn": [{ "target": "build", "projects": "dependencies" }]
    },
    "pl:version": {
      "executor": "nx:run-script",
      "options": {
        "script": "pl:version"
      },
      "dependsOn": [{ "target": "build", "projects": "dependencies" }]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/development-edition-engine-handlebars/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/development-edition-engine-handlebars/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
